{
  "name": "proyecto-calli",
  "version": "0.0.0",
  "author": "Julio CÃ©sar Melchor Pinto",
  "repository": "https://github.com/jcmelchorp/proyecto-calli.git",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "reset": "rm -rf ./node_modules ./package-lock.json && npm install",
    "prod": "ng build --prod",
    "deploy": "ng build --prod && npm run doc:g && firebase deploy",
    "generate:db": "node ./server/generate.js > ./server/database.json",
    "server:db": "npx json-server --watch ./server/database.json",
    "doc:g": "npx @compodoc/compodoc -p ./tsconfig.json --output ./docs --name \"rds-classroom\" --theme material",
    "doc:s": "npx @compodoc/compodoc -p ./tsconfig.json --output ./docs --name \"rds-classroom\" --theme material --serve",
    "doc:w": "npx @compodoc/compodoc -p ./tsconfig.json --output ./docs --name \"rds-classroom\" --theme material --serve --watch",
    "pwa:assets-circle": "npx pwa-asset-generator ./src/assets/icons/1200px-Calli.svg.png ./src/assets/icons/ --manifest ./src/manifest.webmanifest --index ./src/index.html --type png --maskable true --opaque false --favicon",
    "pwa:assets-light": "npx pwa-asset-generator ./src/assets/icons/1200px-Calli.svg.png ./src/assets/icons/white/ --manifest ./src/manifest.webmanifest --index ./src/index.html --type png --maskable true --opaque false --favicon",
    "pwa:assets-std-logo": "npx pwa-asset-generator ./src/assets/icons/1200px-Calli.svg.png ./src/assets/icons/std-logo/ --manifest ./src/manifest.webmanifest --index ./src/index.html --type png --maskable true --opaque false --favicon",
    "pwa:assets-icons": "npx pwa-asset-generator ./src/assets/icons/1200px-Calli.svg.png ./src/assets/icons/ --only-icons --index ./src/index.html --favicon",
    "pwa:audit": "npx lighthouse https://proyecto-cali.web.app --chrome-flags=\"--headless\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
